#!/bin/bash

DIR=operations-vs-latency

for i in {80..560..80}
do
    python ../../benchmark/main.py \
        -type=memcached \
        -server-conf=memcached/memcached.conf \
        -memtier-conf=memcached/memtier.memcached.$i.conf \
        -output=../results/$DIR/memcached/$i/
        -base-port=11120
        -instances=4
        -duration=30
done
