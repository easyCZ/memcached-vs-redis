#!/bin/bash

LABEL=processes
CONNCETIONS=8
mkdir $LABEL

for i in {1..4}
do
    echo "-s nsl200
-p 11120
--test-time=30
-c $CONNCETIONS
-t 1
-P memcache_binary
--random-data
--key-minimum=100
--key-maximum=10000" > $LABEL/memtier.memcached.$(($i + 1)).conf
    CONNCETIONS=$((CONNCETIONS / 2))

done
