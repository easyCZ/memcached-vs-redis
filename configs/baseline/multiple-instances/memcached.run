#!/bin/bash

DIR=multiple-instances

for i in {1..12}
do
    python ../../../benchmark/main.py \
        -type=memcached \
        -server-conf=./memcached.$i.conf \
        -memtier-conf=./memtier.memcached.conf \
        -output=../../../results/baseline/$DIR/$i/ \
        -base-port=11120 \
        -instances=$i \
        -duration=30
done
